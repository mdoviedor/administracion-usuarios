<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Evento" table="evento">
    <id name="idevento" type="integer" column="idevento">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="text" column="nombre" nullable="false"/>
    <field name="descripcion" type="text" column="descripcion" nullable="false"/>
    <field name="desde" type="datetime" column="desde" nullable="false"/>
    <field name="hasta" type="datetime" column="hasta" nullable="false"/>
    <field name="duracion" type="string" column="duracion" length="2" nullable="false"/>
  </entity>
</doctrine-mapping>
